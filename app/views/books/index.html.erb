<p id="notice"><%= notice %></p>

<h1>YearEnd by taichi</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Locate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.name %></td>
        <td><%= book.locate %></td>
        <td><%= link_to 'Show', book %></td>
        <td><%= link_to 'Edit', edit_book_path(book) %></td>
        <td><%= link_to 'Destroy', book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<iframe width="900" height="450" src="https://www.youtube.com/embed/wFKvOPQyVL0" frameborder="0" gesture="media" allow="encrypted-media" allowfullscreen></iframe>
<br>

